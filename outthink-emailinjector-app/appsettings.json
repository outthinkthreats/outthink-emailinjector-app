{
  "app": {
    "name": "OutThink Direct Mail Injection App",
    "service": "EmailInjectorApp",
    "version": "1.0.0"
  },
  "KeyVaultUrl": "<keyvault-url>",
  "ApiBaseUrl": "http://localhost:6000",
  "BatchSize": 10,
  "SkipConfirmation": false,
  "CycleDelay": 60000,
  "AZURE_CLIENT_ID":"<client-id>",
  "AZURE_CLIENT_SECRET":"<client-secret>",
  "AZURE_TENANT_ID":"<tenant-id>",
  "ApplicationInsights": {
    "ConnectionString": "UseEnvironmentVariable:APPLICATIONINSIGHTS_CONNECTION_STRING",
    "EnableAdaptiveSampling": false
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "System": "Warning",
      "OutThink": "Information"
    },
    "ApplicationInsights": {
      "LogLevel": {
        "Default": "Information"
      }
    }
  }
}